    "@types/jquery": ">=3.4.0",
    "@types/json-server": "0.14.0",
    "concurrently": "^4.1.0",
    "http-server": "^0.11.1",
    "nodemon": "^1.19.1",
    "sleep-ms": "^2.0.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "keywords": []
}
